<!-- The properties 'jar', 'outfile', 'fileVersion', 'txtFileVersion', 
     'productVersion' and 'txtProductVersion' are overruled by the 
     installer build.xml file -->

<launch4jConfig>
  <dontWrapJar>false</dontWrapJar>
  <headerType>0</headerType>
  <jar>../geonetwork-2.4.0/geonetwork-install.jar</jar>
  <outfile>../geonetwork-2.4.0/geonetwork-install.exe</outfile>
  <errTitle>GeoNetwork opensource Installation Error</errTitle>
<!--  <downloadUrl>http://java.sun.com/javase/downloads/index_jdk5.jsp</downloadUrl>
  <supportURL>http://java.sun.com/j2se/1.5.0/jre/install-windows.html</supportURL> -->
  <jarArgs></jarArgs>
  <chdir></chdir>
  <customProcName>false</customProcName>
  <stayAlive>false</stayAlive>
  <icon>../installer.ico</icon>
  <jre>
    <path>jre1.5.0_12</path>
    <!-- The minimal version required by the IzPack installer is 1.4
         The JRE for GeoNetwork is included in the installer -->
    <minVersion>1.4.0</minVersion>
<!--    <maxVersion>1.5.0_12</maxVersion> -->
    <initialHeapSize>256</initialHeapSize>
    <maxHeapSize>512</maxHeapSize>
    <args></args>
  </jre>
  <splash>
    <file>../splash.bmp</file>
    <waitForWindow>true</waitForWindow>
    <timeout>60</timeout>
    <timeoutErr>true</timeoutErr>
  </splash>
  <versionInfo>
    <fileVersion>2.4.0.0</fileVersion>
    <txtFileVersion>2.4.0 0</txtFileVersion>
    <fileDescription>GeoNetwork opensource Windows Installer</fileDescription>
    <copyright>OSGeo (http://www.osgeo.org)</copyright>
    <productVersion>2.4.0.0</productVersion>
    <txtProductVersion>2.4.0 0</txtProductVersion>
    <productName>GeoNetwork opensource</productName>
    <companyName></companyName>
    <internalName>geonetwork-install.jar</internalName>
    <originalFilename>geonetwork-install.exe</originalFilename>
  </versionInfo>
</launch4jConfig>